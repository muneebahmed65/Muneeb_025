<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.Muneeb.fypregistrationapplication">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FYPRegistrationApplication"
        tools:targetApi="31">
        <activity
            android:name="com.Muneeb.fypregistrationapplication.AddActivity"
            android:parentActivityName="com.Muneeb.fypregistrationapplication.ViewActivity"
            android:label="Add App."
            android:exported="false" />
        <activity
            android:name="com.Muneeb.fypregistrationapplication.SignupActivity"
            android:parentActivityName="com.Muneeb.fypregistrationapplication.MainActivity"
            android:label="Signup"
            android:exported="false" />
        <activity
            android:name="com.Muneeb.fypregistrationapplication.UpdateActivity"
            android:parentActivityName="com.Muneeb.fypregistrationapplication.ViewActivity"
            android:label="Update/Delete"
            android:exported="false" />
        <activity
            android:name="com.Muneeb.fypregistrationapplication.ViewActivity"
            android:label="FYP Registration App"
            android:exported="false" />
        <activity
            android:name="com.Muneeb.fypregistrationapplication.MainActivity"
            android:label="Login"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>